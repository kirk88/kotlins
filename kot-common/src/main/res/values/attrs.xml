<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StatefulFrameLayout">
        <attr name="emptyLayout" format="reference" />
        <attr name="loadingLayout" format="reference" />
        <attr name="errorLayout" format="reference" />
        <attr name="defaultShow" format="enum">
            <enum name="content" value="0x001" />
            <enum name="empty" value="0x002" />
            <enum name="loading" value="0x003" />
            <enum name="error" value="0x004" />
        </attr>

        <attr name="emptyImage" format="reference" />
        <attr name="emptyText" format="string" />
        <attr name="emptyTextColor" format="color" />
        <attr name="emptyTextAppearance" format="reference" />
        <attr name="emptyButtonText" format="string" />
        <attr name="emptyButtonTextColor" format="color" />
        <attr name="emptyButtonTextAppearance" format="reference" />
        <attr name="emptyButtonBackground" format="reference" />
        <attr name="emptyButtonBackgroundTint" format="color" />
        <attr name="emptyButtonBackgroundTintMode" format="enum" >
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <attr name="emptyButtonVisible" format="boolean" />

        <attr name="errorImage" format="reference" />
        <attr name="errorText" format="string" />
        <attr name="errorTextColor" format="color" />
        <attr name="errorTextAppearance" format="reference" />
        <attr name="errorButtonText" format="string" />
        <attr name="errorButtonTextColor" format="color" />
        <attr name="errorButtonTextAppearance" format="reference" />
        <attr name="errorButtonBackground" format="reference" />
        <attr name="errorButtonBackgroundTint" format="color" />
        <attr name="errorButtonBackgroundTintMode" format="enum" >
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <attr name="errorButtonVisible" format="boolean" />

        <attr name="loadingProgressColor" format="color" />
        <attr name="loadingProgressDrawable" format="reference" />
        <attr name="loadingText" format="string" />
        <attr name="loadingTextColor" format="color" />
        <attr name="loadingTextAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="TitleAppBar">
        <attr name="customLayout" format="reference" />
        <attr name="title" />
        <attr name="titleTextAppearance" />
        <attr name="titleTextColor" />
        <attr name="subtitle" />
        <attr name="subtitleTextAppearance" />
        <attr name="subtitleTextColor" />
        <attr name="titleMargin" />
        <attr name="titleMarginStart" />
        <attr name="titleMarginEnd" />
        <attr name="titleMarginTop" />
        <attr name="titleMarginBottom" />
        <attr name="titleVisible" />
        <attr name="contentInsetStart" />
        <attr name="contentInsetEnd" />
        <attr name="contentInsetLeft" />
        <attr name="contentInsetRight" />
        <attr name="contentInsetStartWithNavigation" />
        <attr name="contentInsetEndWithActions" />
        <attr name="navigationIcon" />
        <attr name="navigationContentDescription" />
        <attr name="navigationIconTint" />
        <attr name="navigationIconTintMode" />
        <attr name="navigationAsUp" format="boolean" />
        <attr name="showBottomDivider" format="enum">
            <enum name="ifNeed" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
        <attr name="bottomDividerHeight" format="dimension|reference" />
        <attr name="bottomDividerColor" format="color|reference" />
        <attr name="provideActionBar" format="boolean" />
        <attr name="popupTheme" format="reference" />
        <attr name="menu" format="reference" />
    </declare-styleable>

    <attr name="titleVisible" format="boolean" />
    <attr name="navigationEnabled" format="boolean" />
    <attr name="navigationIconTint" format="color|reference" />
    <attr name="navigationIconTintMode" format="enum">
        <enum name="src_over" value="3" />
        <enum name="src_in" value="5" />
        <enum name="src_atop" value="9" />
        <enum name="multiply" value="14" />
        <enum name="screen" value="15" />
        <enum name="add" value="16" />
    </attr>

    <attr name="titleAppBarStyle" format="reference" />

    <declare-styleable name="TitleToolbar">
        <attr name="titleTextAppearance" />
        <attr name="subtitleTextAppearance" />
        <attr name="titleVisible" />
        <attr name="navigationIconTint" />
        <attr name="navigationIconTintMode" />
        <attr name="navigationEnabled" />
    </declare-styleable>

    <declare-styleable name="TitleToolbar_Layout">
        <attr name="layout_overrideAttributes" format="boolean" />
    </declare-styleable>

    <declare-styleable name="InfiniteRecyclerView">
        <attr name="android:padding" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="android:paddingStart" />
        <attr name="android:paddingEnd" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingRight" />
        <attr name="android:orientation" />
        <attr name="android:overScrollMode" />
        <attr name="layoutManager" />
        <attr name="spanCount" />
        <attr name="reverseLayout" />
        <attr name="stackFromEnd" />
        <attr name="fastScrollEnabled" />
        <attr name="refreshEnabled" format="boolean" />
        <attr name="loadMoreEnabled" format="boolean" />
    </declare-styleable>

</resources>